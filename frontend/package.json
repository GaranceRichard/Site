{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "perf": "node ./scripts/run-lhci.cjs ./lighthouserc.json",
    "perf:local": "npm run build && npm run perf",
    "perf:dev": "node ./scripts/run-lhci.cjs ./lighthouserc.dev.json",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "lint": "eslint",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:coverage": "cross-env E2E_COVERAGE=true playwright test --workers=1",
    "test:e2e:coverage:report": "npm run test:e2e:coverage && node scripts/e2e-coverage-report.mjs"
  },
  "dependencies": {
    "@sentry/browser": "8.25.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
