{
  "ci": {
    "collect": {
      "url": [
        "http://127.0.0.1:3000/",
        "http://127.0.0.1:3000/contact"
      ],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop",
        "chromeFlags": "--headless=new --no-sandbox --disable-dev-shm-usage"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    }
  }
}
